    "keywords": [
        "eloquent-slug",
        "laravel",
        "eloquet",
        "sluggable",
        "slug"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael G. Boateng",
            "email": "mgyan84@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.4",
        "illuminate/database": "^5.4"        
    },
    "autoload" : {
        "psr-4" : {
            "MGBoateng\\EloquentSlugs\\": "src/"
        }
    },
    "autoload-dev": {
